function perfectNumber(num)
{
    let i=1;
    let sum=0;
    while(i<num)
    {
        if(num%i==0)
        {
            sum=sum+i;
        }
        i++;
    }
    if(num==sum)
    {
        console.log("It is a perfect number");
    }
    else
    {
        console.log("It is not a perfect number");
    }
}
perfectNumber(6);
