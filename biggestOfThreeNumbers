class Main
{
    public static void biggestOf3Numbers(int num1,int num2,int num3)
    {
        if(num1>num2 && num1>num3)
        {
            System.out.println(num1+" is the biggest among all");
        }
        else if(num2>num1 && num2>num3)
        {
            System.out.println(num2+" is the biggest among all");
        }
        else
        {
            System.out.println(num3+" is the biggest among all");
        }
    }

    public static void main(String[] args) {
        biggestOf3Numbers(2003,485,67);
    }
}
